    <profile id="2c07-50b0-e9fb-2503" name="Alchemical Cluster Bombs" publicationId="7b2b-0f56-3962-5ec1" hidden="false" typeId="6c9b-5228-c790-9c90" typeName="Weapon">
      <characteristics>
        <characteristic name="Arc" typeId="8739-aae9-7e50-c8d0">-</characteristic>
        <characteristic name="Point Blank" typeId="4209-eb47-9807-b834">15(7) / 10(5)</characteristic>
        <characteristic name="Closing" typeId="d025-b048-0c22-fad5">-</characteristic>
        <characteristic name="Long" typeId="06b8-7f5e-e2df-751a">-</characteristic>
        <characteristic name="Quality" typeId="faa3-d69c-5156-2acf">Torrent, Alchemical, Bomb</characteristic>
      </characteristics>
    </profile>
    <profile id="93b4-a387-d968-fcfa" name="Alchemical Rockets" publicationId="7b2b-0f56-3962-5ec1" hidden="false" typeId="6c9b-5228-c790-9c90" typeName="Weapon">
      <characteristics>
        <characteristic name="Arc" typeId="8739-aae9-7e50-c8d0">-</characteristic>
        <characteristic name="Point Blank" typeId="4209-eb47-9807-b834">5(1) / 4(-)</characteristic>
